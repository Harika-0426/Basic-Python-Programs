month=int(input("enter the  month:"))
if(month==2):
    print("this monthdays are 28days or 29days,\nmonth is febrauary")
if(month==4 or month==6 or month==9 or month==11):
    print("this monthdays are 30days,\nwith same no. days months are april,june, september,november")
if(month==1 or month==3 or month==5 or month==7 or month==8 or month==10 or month==12):
    print("this monthdays are 31days ,\n with same no. days months are january,march,may,july,august,october,december")
else:
    print("ERROR")
