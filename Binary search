arr=[10,20,30,40,50]
def binary_search(arr,target):
 lb=0
 ub=len(arr)-1
 mid=lb+((lb-ub)//2)
 while (lb<=ub):
     if((arr[mid])==target):
         return mid
     elif((arr[mid])<target):
       lb=mid+1
     else:
       ub=mid-1
 return -1
target=int(input("target:"))
result=binary_search(arr,target)
if(result!=1):
    print("element found at index:",result)
else:
   print("element is not found")
    
